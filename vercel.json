{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build",
          "env": {
            "SERVER": "json-server --watch db.json --port 3001"
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "http://localhost:3001/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }