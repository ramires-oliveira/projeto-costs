{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" },
    { "src": "api/db.json", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://projeto-costs-tau.vercel.app//api/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}